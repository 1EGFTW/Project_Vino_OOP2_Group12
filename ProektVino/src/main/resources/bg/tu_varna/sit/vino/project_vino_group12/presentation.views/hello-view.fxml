<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="bg.tu_varna.sit.vino.project_vino_group12.presentation.controllers.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="HelloText" text="Please choose:"/>
    <Button text="Log in as admin" onAction="#adminLog" fx:id="adminButton"/>
    <Button text="Log in as operator" onAction="#operatorLog" fx:id="operatorButton" />
    <Button text="Log in as warehouse host" onAction="#warehouseHostLog" fx:id="warehouseLogButton"/>
</VBox>
