<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="765" prefWidth="1024" styleClass="wrapper" stylesheets="@css/HelloPage.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane layoutX="60.0" layoutY="60.0" prefHeight="605.0" prefWidth="884.0" styleClass="box">
         <children>
            <VBox layoutX="10.0" prefHeight="499.0" prefWidth="874.0" styleClass="imageNLabel">
               <children>
                  <ImageView fx:id="imageView" fitHeight="513.0" fitWidth="644.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/Logo.png" />
                     </image></ImageView>
                   <Label fx:id="HelloText" prefHeight="16.0" prefWidth="108.0" text="Please choose:" />
               </children>
            </VBox>
            <HBox layoutY="520.0" prefHeight="85.0" prefWidth="884" styleClass="buttonLineup">
                <Button fx:id="adminButton" onAction="#adminLog" styleClass="loginButtons" text="Log in as admin">
                  <graphic>
                     <ImageView fx:id="buttonAdminIcon" fitHeight="26.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true" />
                  </graphic>
                  </Button>
                <Button fx:id="operatorButton" onAction="#operatorLog" styleClass="loginButtons" text="Log in as operator" />
                <Button fx:id="warehouseLogButton" onAction="#warehouseHostLog" styleClass="loginButtons" text="Log in as warehouse host" />
            </HBox>
         </children></Pane>
   </children>
</Pane>
