<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" stylesheets="@../css/InputPages.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane layoutX="137.0" maxHeight="500.0" maxWidth="613.0" minHeight="500.0" minWidth="613.0" prefHeight="500.0" prefWidth="613.0" styleClass="inputField">
         <children>
            <Label fx:id="label1" layoutX="213.0" layoutY="24.0" styleClass="labelTitle" text="Available grapes" />
            <TableView fx:id="grapesTable" layoutX="12.0" layoutY="79.0" prefHeight="421.0" prefWidth="601.0">
               <columns>
                  <TableColumn fx:id="col_Grape" prefWidth="142.0" text="Grape" />
                  <TableColumn fx:id="col_Color" prefWidth="102.0" text="Color" />
                  <TableColumn fx:id="col_Quantity" prefWidth="143.0" text="Available quantity" />
                  <TableColumn fx:id="col_Produced" prefWidth="214.0" text="Wine produced by 1kg" />
               </columns>
            </TableView>
         </children>
      </Pane>
      <Pane prefHeight="500.0" prefWidth="149.0" styleClass="leftMenu">
         <children>
            <Button layoutX="14.0" layoutY="12.0" mnemonicParsing="false" onAction="#goBack" prefHeight="70.0" prefWidth="70.0" styleClass="GoBackButton">
               <graphic>
                  <ImageView fitHeight="79.0" fitWidth="58.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/share-orange.png" />
                     </image></ImageView>
               </graphic></Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
