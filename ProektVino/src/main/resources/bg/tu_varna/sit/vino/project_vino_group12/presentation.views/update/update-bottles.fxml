<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox layoutX="45.0" layoutY="188.0" prefWidth="150.0" fx:id="bottles"/>
      <TextField layoutX="342.0" layoutY="188.0" fx:id="quantity"/>
      <Label layoutX="205.0" layoutY="88.0" text="Choose bottle and enter quantity" />
       <Button fx:id="update" layoutX="240.0" layoutY="354.0" mnemonicParsing="false" onAction="#updateBottles" prefHeight="52.0" prefWidth="132.0" text="Update" wrapText="true"/>
      <Button layoutX="493.0" layoutY="339.0" mnemonicParsing="false" text="Go back" onAction="#goBack"/>
   </children>
</AnchorPane>
