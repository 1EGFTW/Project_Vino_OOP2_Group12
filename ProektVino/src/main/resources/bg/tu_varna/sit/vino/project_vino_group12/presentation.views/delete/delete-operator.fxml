<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" stylesheets="@../css/InputPages.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Pane layoutX="137.0" maxHeight="500.0" maxWidth="613.0" minHeight="500.0" minWidth="613.0" prefHeight="500.0" prefWidth="613.0" styleClass="inputField">
            <children>
                <Label fx:id="label1" layoutX="82.0" layoutY="120.0" styleClass="labelTitle" text="Please choose which operator to delete:" />
                <ComboBox fx:id="operatorComboBox" layoutX="167.0" layoutY="213.0" prefHeight="45.0" prefWidth="280.0" styleClass="combo" />
                <Button fx:id="delete" layoutX="241.0" layoutY="303.0" mnemonicParsing="false" onAction="#deleteOperator" prefHeight="52.0" prefWidth="132.0" styleClass="confirmButtons" text="Delete operator" />
            </children>
        </Pane>
        <Pane prefHeight="500.0" prefWidth="149.0" styleClass="leftMenu">
            <children>
                <Button layoutX="14.0" layoutY="12.0" mnemonicParsing="false" onAction="#goBack" prefHeight="70.0" prefWidth="70.0" styleClass="GoBackButton">
                    <graphic>
                        <ImageView fitHeight="79.0" fitWidth="58.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../images/share-orange.png" />
                            </image></ImageView>
                    </graphic></Button>
            </children>
        </Pane>
    </children>
</AnchorPane>
