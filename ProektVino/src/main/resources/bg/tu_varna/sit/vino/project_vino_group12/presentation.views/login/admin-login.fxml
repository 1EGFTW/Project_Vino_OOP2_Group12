<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" stylesheets="@../css/InputPages.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane layoutX="137.0" maxHeight="500.0" maxWidth="613.0" minHeight="500.0" minWidth="613.0" prefHeight="500.0" prefWidth="613.0" styleClass="inputField">
         <children>
             <Label fx:id="adminlogin" layoutX="194.0" layoutY="165.0" text="Please enter your username and password:" />
            <TextField fx:id="admin_name" layoutX="108.0" layoutY="282.0" prefHeight="37.0" prefWidth="149.0" styleClass="inputFields" />
            <PasswordField fx:id="admin_pass" layoutX="356.0" layoutY="282.0" prefHeight="37.0" prefWidth="149.0" styleClass="inputFields" />
            <Button fx:id="login" layoutX="240.0" layoutY="354.0" mnemonicParsing="false" onAction="#onAdminLoginButtonClick" prefHeight="52.0" prefWidth="132.0" styleClass="confirmButtons" text="Log in as admin" wrapText="true">
               <font>
                  <Font size="10.0" />
               </font></Button>
            <Label layoutX="155.0" layoutY="243.0" text="Username" />
            <Label layoutX="406.0" layoutY="243.0" text="Password" />
         </children>
      </Pane>
     <Pane prefHeight="500.0" prefWidth="149.0" styleClass="leftMenu">
         <children>
            <Button layoutX="14.0" layoutY="12.0" mnemonicParsing="false" onAction="#goBack" prefHeight="70.0" prefWidth="70.0" styleClass="GoBackButton">
               <graphic>
                  <ImageView fitHeight="79.0" fitWidth="58.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/share-orange.png" />
                     </image></ImageView>
               </graphic></Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
